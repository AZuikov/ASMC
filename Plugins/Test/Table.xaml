<UserControl x:Class="Plugins.Test.Table"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Plugins.Test"
             xmlns:view="clr-namespace:ASMC.Core.View;assembly=ASMC.Core"
             xmlns:view1="clr-namespace:ASMC.Common.View;assembly=ASMC.Common"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <view1:InputTableControl Data="{Binding TestData}" SelectValue="{Binding Selected}" /> 
    </Grid>
    
</UserControl>
