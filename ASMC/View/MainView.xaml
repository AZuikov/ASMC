<UserControl x:Class="ASMC.View.MainView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ASMC.View"
             xmlns:ps="http://schemas.palitra-system.ru/winfx/xaml/core"
             xmlns:metr="clr-namespace:ASMC"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:converters="clr-namespace:ASMC.View.Converters"
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <converters:VersionToStringConverter x:Key="VersionToStringConverter" />
    </UserControl.Resources>
    <dxmvvm:Interaction.Behaviors>
        <ps:MessageBoxService />
    </dxmvvm:Interaction.Behaviors>
  <StackPanel>
    <StackPanel>
      <Grid>
        <Grid.ColumnDefinitions>
          <ColumnDefinition/>
          <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="1"></Button>
      </Grid>
    </StackPanel>
    <dxg:GridControl  Height="700"/>
  </StackPanel>

</UserControl>
